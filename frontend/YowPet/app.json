{
  "expo": {
    "name": "YowPet",
    "slug": "YowPet",
    "scheme": "yowpet",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logos/yowpet_fondo.png",
    "userInterfaceStyle": "automatic",
    "description": "Aplicación para gestión y cuidado de mascotas",
    "primaryColor": "#4CAF50",
    "newArchEnabled": true,
     "jsEngine": "jsc",
    "splash": {
      "image": "./assets/logos/yowpet.png",
      "resizeMode": "contain",
      "backgroundColor": "#0E333C"
    },
    "ios": {
      "supportsTablet": true,
      "icon": "./assets/logos/yowpet.png"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logos/yowpet.png",
        "backgroundColor": "#ffffff",
        "package": "com.anonymous.YowPet",
        "googleMapsApiKey": "AIzaSyDK3339yIBqQ3b7obl2pOmMJpByzsKUt2Q"
      },
      "permissions": [
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.INTERNET",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.INTERNET",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ],
      "package": "com.anonymous.YowPet"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/logos/icon.png",
      "name": "YowPet - Cuiado de Mascotas"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-file-system"
    ],
    "assetBundlePatterns": [
      "**/*"
    ],
    "experiments": {
      "tsconfigPaths": true
    }
  }
}