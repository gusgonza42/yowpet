    "@tanstack/react-query": "^5.71.5",
    "axios": "^1.8.4",
    "expo": "~52.0.42",
    "expo-constants": "~17.0.8",
    "expo-file-system": "~18.0.12",
    "expo-linking": "~7.0.5",
    "expo-router": "~4.0.20",
    "expo-secure-store": "~14.0.1",
    "expo-status-bar": "~2.0.1",
    "formik": "^2.4.6",
